{"components/common/currency-selector":"{{#if currency_selector.currencies.length '>' 1}}\n<ul class=\"navUser-section\">\n    <li class=\"navUser-item\">\n        <a class=\"navUser-action navUser-action--currencySelector has-dropdown\" href=\"#\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\">{{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i></a>\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n            {{#each currency_selector.currencies}}\n            <li class=\"dropdown-menu-item\">\n                <a href=\"{{{switch_url}}}\">\n                    {{#if is_active}}\n                    <strong>{{name}}</strong>\n                    {{else}}\n                    {{name}}\n                    {{/if}}\n                </a>\n            </li>\n            {{/each}}\n        </ul>\n    </li>\n</ul>\n{{/if}}\n","components/common/navigation":"<nav class=\"navUser\">\n    {{> components/common/currency-selector}}\n\n    <ul class=\"navUser-section navUser-section--alt\">\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navUser-item\">\n                <a class=\"navUser-action navUser-action--storeCredit\"\n                   data-dropdown=\"storeCredit-dropdown\"\n                   data-options=\"align:bottom\"\n                   href=\"{{urls.cart}}\">\n                        <span class=\"navUser-action-divider\">{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}</span>\n                </a>\n                <div class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\">\n                    {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\n                </div>\n            </li>\n        {{/if}}\n        <li class=\"navUser-item\">\n            <a class=\"navUser-action navUser-item--compare\" href=\"{{urls.compare}}\" data-compare-nav>{{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span></a>\n        </li>\n        {{#if theme_settings.social_icon_placement_top}}\n            <li class=\"navUser-item navUser-item--social\">\n                {{> components/common/social-links}}\n            </li>\n            <li class=\"navUser-item navUser-item--divider\">|</li>\n        {{/if}}\n        <li class=\"navUser-item\">\n            <a class=\"navUser-action navUser-action--quickSearch\" href=\"#\" data-search=\"quickSearch\" aria-controls=\"quickSearch\" aria-expanded=\"false\">{{lang 'common.search'}}</a>\n        </li>\n        {{#if settings.gift_certificates_enabled}}\n            <li class=\"navUser-item\">\n                <a class=\"navUser-action\" href=\"{{urls.gift_certificate.purchase}}\">{{lang 'common.gift_cert'}}</a>\n            </li>\n        {{/if}}\n        <li class=\"navUser-item navUser-item--account\">\n            {{#if customer}}\n                <a class=\"navUser-action\" href=\"{{urls.account.index}}\">{{lang 'common.account'}}</a>\n                <a class=\"navUser-action\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\n            {{else}}\n                <a class=\"navUser-action\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a>\n                {{#if settings.account_creation_enabled}}\n                    <span class=\"navUser-or\">{{lang 'common.or'}}</span> <a class=\"navUser-action\" href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\n                {{/if}}\n            {{/if}}\n        </li>\n        <li class=\"navUser-item navUser-item--cart\">\n            <a\n                class=\"navUser-action\"\n                data-cart-preview\n                data-dropdown=\"cart-preview-dropdown\"\n                data-options=\"align:right\"\n                href=\"{{urls.cart}}\">\n                <span class=\"navUser-item-cartLabel\">\n                    {{lang 'common.cart'}}\n                   <!--  <img src=\"{{cdn 'assets/img/cart.png'}}\" class=\"cart_icon\"> -->\n                </span> \n                <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>\n            </a>\n\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n        </li>\n    </ul>\n    <div class=\"dropdown dropdown--quickSearch\" id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\n        {{> components/common/quick-search}}\n    </div>\n</nav>\n","components/common/quick-search":"\n    {{{ snippet \"forms_search\"}}}\n    <form class=\"form\" action=\"{{urls.search}}\">\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"{{lang 'search.results.form_button_text'}}\">\n            </div>\n        </fieldset>\n    </form>\n    <section class=\"quickSearchResults search_popup\" data-bind=\"html: results\"></section>\n\n","components/common/social-links":"{{#if social_media}}\n<ul class=\"socialLinks socialLinks--alt\">\n    {{#each social_media}}\n    <li class=\"socialLinks-item\">\n        {{#if display_name '===' 'facebook'}}\n            <a class=\"icon icon--facebook\" href=\"{{url}}\" target=\"_blank\">\n                 <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"15.219\" height=\"33.16\" viewBox=\"0 0 15.219 33.16\">\n                      <metadata><?xpacket beg